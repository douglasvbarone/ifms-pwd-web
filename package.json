{
  "name": "ifms-pwd-web",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev:web": "vite --host",
    "dev:server": "tsx --watch ./src/server/index.ts",
    "dev": "NODE_ENV=development concurrently --kill-others -n 'Server,Web   ' -c bgGreen,bgBlue \"npm run dev:server\" \"npm run dev:web\"",
    "build:web": "vue-tsc --noEmit && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run clean && npm run build:web && npm run build:server",
    "preview:web": "vite preview --host",
    "preview": "npm run build && npm start",
    "start": "NODE_ENV=production node dist/server"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ldapts": "^7.0.7",
    "roboto-fontface": "*",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.15.0",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
